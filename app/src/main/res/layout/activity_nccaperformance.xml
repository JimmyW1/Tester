            android:id="@+id/btn_sm4_ecb1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/btn_perform_sm4_ecb_1" />

        <Button
            android:id="@+id/btn_sm4_ecb2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/btn_perform_sm4_ecb_2" />

        <Button
            android:id="@+id/btn_sm4_ecb3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/btn_perform_sm4_ecb_3" />
    </LinearLayout>

</LinearLayout>
